		{
			"name": "Users",
			"item": [
				{
					"name": "List users",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"const jsonData = pm.response.json();\r",
									"\r",
									"pm.test(\"Status code is 200 OK\", function () {\r",
									"    pm.response.to.have.status(200);\r",
									"});\r",
									"\r",
									"pm.test(\"Verify page number\", function () {\r",
									"    pm.expect(jsonData.page).to.eql(1);\r",
									"});\r",
									"\r",
									"pm.test(\"Verify 'data' property exists\", function () {\r",
									"    pm.expect(jsonData).to.have.property(\"data\");\r",
									"});\r",
									"\r",
									"pm.test(\"Verify user\", function () {\r",
									"    let user = jsonData.data.find(x => x.id == 5)\r",
									"    pm.expect(user.first_name).eq(\"Charles\");\r",
									"    console.log(user)\r",
									"})"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "GET",
						"header": [],
						"url": {
							"raw": "{{host}}/api/users?page=1",
							"host": [
								"{{host}}"
							],
							"path": [
								"api",
								"users"
							],
							"query": [
								{
									"key": "page",
									"value": "1"
								}
							]
						}
					},
					"response": []
				},
